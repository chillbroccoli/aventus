{
  "name": "database",
  "version": "1.0.0",
  "author": "",
  "license": "ISC",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup index.ts --format cjs --dts",
    "dev": "tsup index.ts --format esm,cjs --watch --dts",
    "db:generate": "dotenv -e ../../.env prisma generate",
    "db:push": "dotenv -e ../../.env prisma db push --skip-generate",
    "db:seed": "dotenv -e ../../.env prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^4.10.1"
  },
  "devDependencies": {
    "dotenv-cli": "latest",
    "prisma": "^4.10.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.6.3",
    "eslint-config-custom": "*",
    "tsconfig": "*"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
